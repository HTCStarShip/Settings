    </com.htc.preference.HtcPreference>
    <com.htc.preference.HtcPreferenceScreen android:title="@string/proxy_settings_title" android:key="proxy_settings" android:fragment="com.android.settings.ProxySelector" />
    <com.htc.preference.HtcCheckBoxPreference android:title="@string/PS" android:key="toggle_PS" android:summary="@string/PS_summary" />
</com.htc.preference.HtcPreferenceScreen>